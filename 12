#pragma warning (disable:4996)
#include <stdio.h>
#include <math.h>
main() {
  const float Pi = 3.141, R = 1000;
  float m, l, d; 
  scanf("%f %f", &m, &l);
  if (m > 0 && l > 0) {
    d = 2 * sqrt(m / (R * l * Pi));
    printf("2 * sqrt(%.3f / (%.3f * %.3f * %.3f) = %f", m, R, l, Pi, d);
  }
  else {
    while (m <= 0 || l <= 0) {
      printf("select another values\n");
      scanf("%f %f", &m, &l);
      if (m > 0 && l > 0) {
        d = 2 * sqrt(m / (R * l * Pi));
        printf("2 * sqrt(%.3f / (%.3f * %.3f * %.3f) = %f", m, R, l, Pi, d);
      }
    }

  }
  return 0;
}
